{
	"name": "wapplersystems/videos",
	"type": "typo3-cms-extension",
	"description": "Extends video file properties and provides a player for playlists, cue points and subtitles",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"TYPO3",
		"extension",
		"video",
		"video",
		"videos",
		"playlist",
		"markers",
		"cue points",
		"tracks"
	],
	"homepage": "https://wappler.systems",
	"require": {
		"typo3/cms-core": "^12",
		"typo3/cms-filemetadata": "^12"
	},
	"autoload": {
		"psr-4": {
			"WapplerSystems\\Videos\\": "Classes"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "videos"
		}
	}
}
